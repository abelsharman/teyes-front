{
  "name": "teyes-front",
  "version": "2.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "npm": "^8.19.2",
    "node": "^16.18.1"
  },
  "scripts": {
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "test:unit": "vitest run --environment jsdom --passWithNoTests",
    "test:watch": "vitest --environment jsdom",
    "format:check": "prettier --check '**/*.{vue,cjs,mjs,cts,mts,js,jsx,ts,tsx,json,css,md,yml}'",
    "format": "prettier --write '**/*.{vue,cjs,mjs,cts,mts,js,jsx,ts,tsx,json,css,md,yml}'",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "dev": "cross-env NODE_ENV=development nodemon server/index.js",
    "build:production": "vite build -m production && vite build --ssr -m production",
    "build:dev": "vite build -m development && vite build --ssr -m development",
    "build:staging": "vite build -m staging && vite build --ssr -m staging",
    "start-server": "cross-env NODE_ENV=production node server/index.js",
    "start-server:demon": "cross-env NODE_ENV=production nodemon server/index.js"
  },
  "dependencies": {
    "@fastify/cookie": "9.0.4",
    "@fastify/middie": "8.3.0",
    "@fastify/static": "6.11.0",
    "@headlessui/tailwindcss": "0.1.3",
    "@headlessui/vue": "1.7.13",
    "@types/node": "^15.14.9",
    "@vue/server-renderer": "3.3.4",
    "autoprefixer": "9",
    "axios": "1.5.1",
    "body-scroll-lock": "^4.0.0-beta.0",
    "convict": "6.2.4",
    "core-js": "^3.6.5",
    "cors": "2.8.5",
    "dotenv": "16.1.3",
    "express": "4.18.2",
    "express-useragent": "1.0.15",
    "fastify": "4.21.0",
    "fastify-plugin": "4.5.1",
    "hot-shots": "10.0.0",
    "izitoast": "^1.4.0",
    "js-cookie": "3.0.5",
    "lodash-es": "4.17.21",
    "lru-cache": "10.0.1",
    "morgan": "1.10.0",
    "nouislider": "15.7.0",
    "nprogress": "0.2.0",
    "pinia": "2.0.36",
    "postcss": "7",
    "sirv": "2.0.3",
    "styled-components": "^5.3.11",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "ts-node": "10.9.1",
    "ts-toolbelt": "9.6.0",
    "typescript": "^4.9.5",
    "vite-plugin-ssr": "0.4.140",
    "vue": "3.3.4",
    "vue-router": "4.1.6",
    "vue-scrollto": "^2.20.0",
    "vue3-carousel": "^0.3.1",
    "vue-yandex-maps": "^1.0.8"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "17.4.2",
    "@commitlint/config-conventional": "17.4.2",
    "@rushstack/eslint-patch": "1.1.4",
    "@tailwindcss/typography": "0.5.9",
    "@types/cors": "2.8.13",
    "@types/express": "4.17.17",
    "@types/express-useragent": "1.0.2",
    "@types/glidejs__glide": "3.6.1",
    "@types/js-cookie": "3.0.3",
    "@types/jsdom": "20.0.1",
    "@types/lodash-es": "4.17.6",
    "@types/node": "18.11.12",
    "@typescript-eslint/eslint-plugin": "5.52.0",
    "@vitejs/plugin-vue": "4.2.3",
    "@vue/cli-plugin-babel": "~4.5.13",
    "@vue/cli-plugin-router": "~4.5.13",
    "@vue/cli-plugin-vuex": "~4.5.13",
    "@vue/cli-service": "~4.5.13",
    "@vue/eslint-config-prettier": "7.0.0",
    "@vue/eslint-config-typescript": "11.0.0",
    "@vue/test-utils": "2.2.6",
    "@vue/tsconfig": "0.1.3",
    "autocannon": "7.12.0",
    "autoprefixer": "10.4.13",
    "cross-env": "7.0.3",
    "eslint": "8.22.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-vue": "9.3.0",
    "jsdom": "20.0.3",
    "lint-staged": "13.1.0",
    "nodemon": "2.0.22",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.21",
    "prettier": "2.7.1",
    "prettier-plugin-tailwindcss": "0.2.3",
    "tailwindcss": "3.3.3",
    "typescript": "4.9.5",
    "vite": "4.3.9",
    "vite-plugin-html": "3.2.0",
    "vitest": "0.34.2",
    "vue-cli-plugin-axios": "~0.0.4",
    "vue-cli-plugin-tailwind": "~3.0.0",
    "vue-template-compiler": "^2.6.11",
    "vue-tsc": "1.6.5",
    "yargs-parser": "21.1.1"
  }
}
