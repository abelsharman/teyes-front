{
  "name": "vue-project",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev:mobile": "VITE_APP_NAME=mobile vite --host",
    "dev:desktop": "VITE_APP_NAME=desktop vite --host",
    "build": "run-p type-check build-only",
    "build-only:production": "npm-run-all --parallel build-only:desktop build-only:mobile",
    "build-only:desktop": "VITE_APP_NAME=desktop vite build --mode production --emptyOutDir ",
    "build-only:mobile": "VITE_APP_NAME=mobile vite build --mode production --emptyOutDir",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-useragent": "^1.0.15",
    "morgan": "^1.10.0",
    "vue": "^3.3.4",
    "vue-router": "^4.2.2",
    "vue-scrollto": "^2.20.0",
    "vue-yandex-maps": "^1.0.8",
    "vue3-carousel": "^0.3.1"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.9",
    "@tsconfig/node18": "^2.0.1",
    "@types/node": "^18.16.18",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.14",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.25",
    "tailwindcss": "^3.3.3",
    "typescript": "~5.0.4",
    "vite": "^4.3.9",
    "vue-tsc": "^1.6.5"
  }
}
